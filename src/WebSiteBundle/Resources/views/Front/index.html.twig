                        <p class="text-forfait">
                            {{ 'tarifs.parcours4'|trans({'%poids%': tarifs.F4Poids })|raw }}
                        </p>
                    </div>
                </div>
                <div class="col-md-6 col-sm-8 col-sm-offset-2 col-md-offset-0">
                    <h3 class="text-center text-yellow">
